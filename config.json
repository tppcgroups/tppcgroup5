{
  "project": {
    "name": "tampapalms",
    "displayName": "Tampa Palms Professional Center",
    "description": "Next.js 15 App Router site for the Tampa Palms Professional Center leasing and marketing pages.",
    "framework": "nextjs",
    "appRouter": true,
    "language": "typescript",
    "packageManager": "npm",
    "usesTurbopack": true,
    "sourceDir": "src",
    "publicDir": "public",
    "buildDir": ".next"
  },
  "branding": {
    "siteTitle": "Tampa Palms Professional Center - TPPC",
    "metaDescription": "Website for TPPC",
    "logo": "/images/TampaPalmsLogo.png",
    "favicon": "/favicon.ico"
  },
  "contact": {
    "office": {
      "street": "17427 Bridge Hill Ct STE C",
      "city": "Tampa",
      "state": "FL",
      "zip": "33647",
      "mapUrl": "https://maps.google.com/?q=17427+Bridge+Hill+Ct+STE+C,+Tampa,+FL+33647"
    },
    "phone": {
      "primary": "(813) 876-7697",
      "tel": "8138767697",
      "emergency": "813-876-7697"
    },
    "email": {
      "marketing": "marketing@tampapalmscenter.com",
      "marketingFallback": "marketing@tampapalms.com"
    },
    "social": {
      "linkedin": "https://www.linkedin.com/company/soarco-working/",
      "facebook": "https://www.facebook.com/TampaPalmsProfessionalCenter",
      "instagram": "https://www.instagram.com/tampapalmsprofessionalcenter/"
    },
    "visitingHours": [
      { "label": "Mon – Thu", "value": "9:00 am – 5:00 pm" },
      { "label": "Fri", "value": "9:00 am – 3:00 pm" }
    ]
  },
  "navigation": {
    "headerLinks": [
      { "label": "Home", "href": "/" },
      { "label": "Availability", "href": "/pages/Availability" },
      { "label": "Features & Amenities", "href": "/pages/Features" },
      {
        "label": "Services",
        "children": [
          { "label": "Maintenance", "href": "/pages/Maintenance" },
          { "label": "Apply", "href": "/pages/Apply" }
        ]
      },
      { "label": "Testimonials", "href": "/pages/Testimonials" },
      { "label": "About", "href": "/pages/About" },
      { "label": "Contact", "href": "/pages/Contact" }
    ],
    "footerQuickLinks": [
      { "label": "Home", "href": "/pages/Home" },
      { "label": "Availability", "href": "/pages/Availability" },
      { "label": "Features & Amenities", "href": "/pages/Features" },
      { "label": "Maintenance Request", "href": "/pages/Maintenance" },
      { "label": "Rental Application", "href": "/pages/Apply" },
      { "label": "Testimonials", "href": "/pages/Testimonials" },
      { "label": "About", "href": "/pages/About" },
      { "label": "Contact", "href": "/pages/Contact" }
    ]
  },
  "api": {
    "routes": [
      { "path": "/api/buildings", "method": "GET", "purpose": "Supabase read of buildings inventory and availability." },
      { "path": "/api/contact", "method": "POST", "purpose": "Contact form submission, Supabase insert, and nodemailer confirmations." },
      { "path": "/api/notify-request", "method": "POST", "purpose": "Create notify_requests rows when users ask for suite availability alerts." },
      { "path": "/api/completed-notify", "method": "GET", "purpose": "Cron-triggered availability notifier; sends emails and marks notify requests complete." },
      { "path": "/api/email", "method": "POST", "purpose": "Reusable transactional/marketing email sender for availability and marketing templates." },
      { "path": "/api/testimonials", "method": "GET", "purpose": "Fetch testimonials from Supabase." },
      { "path": "/api/faqs", "method": "GET", "purpose": "Fetch FAQs from Supabase." },
      { "path": "/api/users", "method": "GET", "purpose": "Administrative listing of users table entries." },
      { "path": "/api/unsubscribe", "method": "GET", "purpose": "Unsubscribe token handler used in email links." },
      { "path": "/api/accessibility-events", "method": "POST", "purpose": "Log accessibility events." },
      { "path": "/api/image-map", "method": "POST", "purpose": "Transform relative image paths to absolute URLs." }
    ],
    "cron": {
      "vercel": [
        {
          "path": "/api/completed-notify",
          "schedule": "*/15 * * * *",
          "purpose": "Poll notify_requests and email users when suites become available."
        }
      ]
    }
  },
  "integrations": {
    "supabase": {
      "urlKey": "NEXT_PUBLIC_SUPABASE_URL",
      "anonKey": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
      "serviceRoleKey": "SUPABASE_SERVICE_ROLE_KEY",
      "tables": [
        "buildings",
        "users",
        "contact_us",
        "notify_requests",
        "completed_notify",
        "faqs",
        "testimonials"
      ]
    },
    "email": {
      "transport": "nodemailer",
      "smtpKeys": {
        "host": "SMTP_HOST",
        "port": "SMTP_PORT",
        "secure": "SMTP_SECURE",
        "user": "EMAIL_USER",
        "pass": "EMAIL_PASSWORD"
      },
      "fromAddressKey": "EMAIL_FROM_ADDRESS",
      "internalNotificationKey": "EMAIL_INTERNAL_NOTIFICATIONS",
      "siteUrlKeys": ["NEXT_PUBLIC_SITE_URL", "SITE_URL"]
    },
    "marketing": {
      "publicEmailKey": "NEXT_PUBLIC_MARKETING_EMAIL",
      "backendEmailKey": "MARKETING_EMAIL",
      "logoUrlKeys": ["MARKETING_LOGO_URL", "NEXT_PUBLIC_LOGO_URL"],
      "subscribeUrlKeys": ["NOTIFY_SUBSCRIBE_URL", "NEXT_PUBLIC_NOTIFY_SUBSCRIBE_URL"]
    }
  },
  "env": {
    "required": {
      "SUPABASE_URL": "",
      "SUPABASE_SERVICE_ROLE_KEY": "",
      "NEXT_PUBLIC_SUPABASE_URL": "",
      "NEXT_PUBLIC_SUPABASE_ANON_KEY": "",
      "EMAIL_USER": "",
      "EMAIL_PASSWORD": "",
      "SMTP_HOST": "smtp.gmail.com",
      "SMTP_PORT": "587",
      "SMTP_SECURE": "false",
      "EMAIL_FROM_ADDRESS": "",
      "EMAIL_INTERNAL_NOTIFICATIONS": "marketing@tampapalmscenter.com",
      "NEXT_PUBLIC_SITE_URL": "http://localhost:3000",
      "SITE_URL": "",
      "MARKETING_EMAIL": "marketing@tampapalms.com",
      "NEXT_PUBLIC_MARKETING_EMAIL": "marketing@tampapalms.com",
      "MARKETING_LOGO_URL": "",
      "NEXT_PUBLIC_LOGO_URL": "",
      "NOTIFY_SUBSCRIBE_URL": "https://tppcgroup5.vercel.app/pages/DeleteEmail",
      "NEXT_PUBLIC_NOTIFY_SUBSCRIBE_URL": "https://tppcgroup5.vercel.app/pages/DeleteEmail"
    }
  },
  "assets": {
    "heroCarousel": [
      "/images/Bldg5-003.jpg",
      "/images/Bldg6-001.jpg",
      "/images/5331/5331-Primrose-Lake-Cir-Tampa-FL-Aerial-1-LargeHighDefinitionEdit.png",
      "/images/Bldg6-007.jpg"
    ],
    "logo": "/images/TampaPalmsLogo.png"
  },
  "features": {
    "accessibilityWidget": true,
    "toastNotifications": true
  }
}
